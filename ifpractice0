
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int a = 0;
		a = scn.nextInt();
		if(a%4==0 && a%100!=0) {
			System.out.println("1");
		}else if(a%4==0 && a%400==0) {
			System.out.println("1");
		}else {
			System.out.print("0");
		}
	}
}
